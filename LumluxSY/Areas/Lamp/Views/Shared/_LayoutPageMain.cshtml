                                            async: false,//异步提交
                                            url: 'https://ditu.google.cn/maps/api/js?key=AIzaSyAc1YixAJuZBs7nnzCfBAxbwMKYexIU7sw&sensor=true',
                                            dataType: 'Json',
                                            success: function (data2) {
                                                $.ajax({
                                                    type: "POST",
                                                    cache: false,//拒绝缓存
                                                    async: false,//异步提交
                                                    url: '/Lamp/Main/ConfigMap?map=GoogleMap',
                                                    data: { map: "GoogleMap" },
                                                    dataType: 'Json',
                                                    success: function (data2) {
                                                        location.reload(true);
                                                    },
                                                    error: function (error) {
                                                        location.href = '/Lamp/User/Login';
                                                    }
                                                })
                                            },
                                            error: function (error) {
                                                if (error.status == 200 || error.status == 503) {
                                                    $.ajax({
                                                        type: "POST",
                                                        cache: false,//拒绝缓存
                                                        async: false,//异步提交
                                                        url: '/Lamp/Main/ConfigMap?map=GoogleMap',
                                                        data: { map: "GoogleMap" },
                                                        dataType: 'Json',
                                                        success: function (data2) {
                                                            location.reload(true);
                                                        },
                                                        error: function (error) {
                                                            location.href = '/Lamp/User/Login';
                                                        }
                                                    })
                                                }
                                                else {
                                                    alert("无法访问GooGle地图，停止切换");
                                                }
                                            }
                                        })
                                    }
                                    function ToAMap() {
                                        $.ajax({
                                            type: "GET",
