                                            dataType: 'Json',
                                            success: function (data2) {
                                                $.ajax({
                                                    type: "POST",
                                                    cache: false,//拒绝缓存
                                                    async: false,//异步提交
                                                    url: '/Lamp/Main/ConfigMap?map=AMap',
                                                    data: { map: "AMap" },
                                                    dataType: 'Json',
                                                    success: function (data2) {
                                                        location.reload(true);
                                                    },
                                                    error: function (error) {
                                                        location.href = '/Lamp/User/Login';
                                                    }
                                                })
                                            },
                                            error: function (error) {
                                                if (error.status == 200) {
                                                    $.ajax({
                                                        type: "POST",
                                                        cache: false,//拒绝缓存
                                                        async: false,//异步提交
